{
	"id": "page",
	"label": "Page",
	"format": "page",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID",
					"placeholder": ""
				}
			},
			"title": {
				"type": "StructuredText",
				"config": {
					"label": "Title",
					"placeholder": "",
					"allowTargetBlank": true,
					"single": "heading1"
				}
			},
			"background": {
